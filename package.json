{
  "name": "ttrp-server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Axel Elgenstierna",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "start": "node",
    "build": "npm i --oly=dev tsc",
    "db:rollback": "knex --knexfile ./src/knexfile.ts migrate:rollback",
    "db:migrate": "knex --knexfile ./src/knexfile.ts migrate:latest",
    "db:seed": "knex --knexfile ./src/knexfile.ts seed:run",
    "db:boot": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "knex": "^1.0.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@types/prettier": "^2.4.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.2"
  }
}
